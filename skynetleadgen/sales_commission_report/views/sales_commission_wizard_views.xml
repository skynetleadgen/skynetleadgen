<odoo>
  <record id="view_sales_commission_wizard_form" model="ir.ui.view">
    <field name="name">sales.commission.wizard.form</field>
    <field name="model">sales.commission.wizard</field>
    <field name="arch" type="xml">
      <form string="Sales Commission Report">
        <group>
          <field name="month"/>
        </group>
        <footer>
          <button string="Print"
                  type="object"
                  name="action_print"
                  class="btn-primary"/>
          <button string="Cancel" special="cancel" class="btn-secondary"/>
        </footer>
      </form>
    </field>
  </record>

  <record id="action_sales_commission_wizard" model="ir.actions.act_window">
    <field name="name">Sales Commission Report</field>
    <field name="res_model">sales.commission.wizard</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
  </record>

  <menuitem id="menu_sales_commission_root"
            name="Sales Commissions"
            parent="base.menu_custom"
            sequence="90"/>

  <menuitem id="menu_sales_commission_report"
            name="Commission Report"
            parent="menu_sales_commission_root"
            action="action_sales_commission_wizard"
            sequence="10"/>
</odoo>
